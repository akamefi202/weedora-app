{
  "expo": {
    "name": "weedora",
    "slug": "weedora",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/app_icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#034638"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.weedora.com",
      "buildNumber": "5.0.0"
    },
    "android": {
      "package": "app.weedora.com"
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}